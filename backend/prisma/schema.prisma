datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Registration {
  id        String   @id @default(uuid())
  aadhaar   String   @unique
  pan       String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}